{
    "env": {
        "node": true,
        "mocha": true
    },
    "plugins": [
        "es5"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:es5/no-es2015"
      ],
    "rules": {
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "no-var": 0,
        "no-underscore-dangle": 0,
        "vars-on-top": 0
    }
}